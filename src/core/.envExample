# Django
SECRET_KEY=DJANGO SECRET KEY

DEBUG="0" or "1"

DJANGO_ALLOWED_HOSTS=HOSTS, DELIMITER=SPACE, LIKE "123.0.0.0 localhost [::1]

CORS_ALLOWED=EXTERNAL FRONTEND APP HOST

# Email
EMAIL_PWD=EMAIL_SMTP_PASSWORD
EMAIL_HOST=EMAIL_HOST
EMAIL_PORT=EMAIL_PORT
